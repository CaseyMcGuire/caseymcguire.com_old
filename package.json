{
  "name": "caseymcguire.com",
  "version": "1.0.0",
  "description": "My website",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "clean": "node_modules/trash-cli/cli.js dist",
    "build-client": "node_modules/webpack-cli/bin/cli.js",
    "build-server": "node_modules/typescript/bin/tsc",
    "copy-static-files": "node_modules/gulp/bin/gulp.js copy-static-files",
    "watch-client": "node_modules/webpack-cli/bin/cli.js -w --mode=development",
    "watch-server": "node_modules/typescript/bin/tsc -w",
    "watch-handlebars": "node_modules/gulp/bin/gulp.js watch-handlebars",
    "watch-assets": "node_modules/gulp/bin/gulp.js watch-assets",
    "copy-fonts": "node_modules/gulp/bin/gulp.js copy-fonts",
    "watch-dev": "concurrently -k npm:watch-client npm:watch-server npm:watch-handlebars npm:watch-assets",
    "start-server": "node_modules/nodemon/bin/nodemon.js --watch dist/server --watch dist/shared dist/server/index.js",
    "db-migrate": "node_modules/db-migrate/bin/db-migrate --config ./src/server/config/database.json --migrations-dir src/server/db/migrations",
    "setup-database": "npm run db-migrate up",
    "build-prod": "npm install && npm run build-client && npm run build-server && npm run copy-static-files && npm run setup-database",
    "heroku-postbuild": "npm run build-prod"
  },
  "author": "Casey McGuire",
  "license": "ISC",
  "dependencies": {
    "ace-builds": "^1.4.1",
    "bcryptjs": "^2.4.3",
    "brace": "^0.11.1",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "immutable": "^3.8.2",
    "marked": "^0.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "typescript": "^2.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/marked": "^0.4.0",
    "@types/node": "^10.3.2",
    "@types/pg": "^7.4.10",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/bcryptjs": "^2.4.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express-handlebars": "0.0.30",
    "@types/express-session": "^1.15.10",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "concurrently": "^3.6.0",
    "css-loader": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-watch": "^5.0.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "trash-cli": "^1.4.0",
    "ts-loader": "^4.4.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
